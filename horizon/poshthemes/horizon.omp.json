{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "osc99": false,
  "console_title": false,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "horizontal_offset": 0,
      "vertical_offset": 0,
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "lightBlue",
          "properties": {
            "prefix": "",
            "postfix": " ",
            "macos": "mac"
          }
        },
        {
          "type": "path",
          "style": "plain",
          "invert_powerline": false,
          "foreground": "lightGreen",
          "properties": {
            "home_icon": "~",
            "folder_icon": "..",
            "folder_separator_icon": "/",
            "style": "agnoster_short"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "lightYellow",
          "properties": {
            "prefix":"on ",
            "postfix":" ",
            "local_working_icon": "",
            "local_staged_icon": "",
            "stash_count_icon": "↪",
            "status_separator_icon": " |",
            "tag_icon": "▶ ",
            "branch_icon": " ",
            "branch_identical_icon": "=",
            "branch_gone_icon": "!=",
            "branch_ahead_icon": "⬆",
            "branch_behind_icon": "⬇",
            "display_stash_count": true,
            "display_upstream_icon": false
          }
        },
        {
          "type": "python",
          "style": "powerline",
          "foreground": "red",
          "properties": {
            "prefix": "\u005b\uE235 ",
            "postfix": "\u005D ",
            "display_version": false,
            "display_virtual_env": true,
            "display_mode": "environment"
          }
        },
        {
          "type": "go",
          "style": "powerline",
          "foreground": "red",
          "properties": {
            "prefix": "\u005b\uFCD1 ",
            "postfix": "\u005D "
          }
        },
        {
          "type": "node",
          "style": "powerline",
          "foreground": "red",
          "properties": {
            "prefix": "\u005b\uE718 ",
            "postfix": "\u005D "
          }
        },
        {
          "type": "ruby",
          "style": "powerline",
          "foreground": "red",
          "properties": {
            "prefix": "\u005b\uE791",
            "postfix": "\u005D "
          }
        },
        {
          "type": "java",
          "style": "powerline",
          "foreground": "red",
          "properties": {
            "prefix": "\u005b\uE738 ",
            "postfix": "\u005D "
          }
        },
        {
          "type": "julia",
          "style": "powerline",
          "foreground": "red",
          "properties": {
            "prefix": " \uE624 "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments":[
        {
          "type": "exit",
          "style": "plain",
          "foreground": "lightRed",
          "properties":{
            "postfix": " ]",
            "prefix": "[ "
          }
        }
      ]
      },
      {
        "type": "newline"
      },
      {
        "type": "prompt",
        "alignment": "left",
        "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "yellow",
          "properties": {
              "prefix": "",
              "text": " \u279C"
          }
        }
        ]
      }
  ],
  "final_space": true
}
